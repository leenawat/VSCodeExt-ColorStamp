{
	"name": "color-bar",
	"displayName": "color-bar",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.30.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.colorBar",
		"onCommand:extension.colorBarBlue",
		"onCommand:extension.colorBarRed",
		"onCommand:extension.colorBarGreen",
		"onCommand:extension.colorBarYellow",
		"onCommand:extension.colorBarOrange",
		"onCommand:extension.colorBarPurple",
		"onCommand:extension.colorBarSilver",
		"onCommand:extension.colorBarKhaki"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.colorBar",
				"title": "Color Title Bar : ?"
			},
			{
				"command": "extension.colorBarBlue",
				"title": "Color Title Bar : Blue"
			},
			{
				"command": "extension.colorBarRed",
				"title": "Color Title Bar : Red"
			},
			{
				"command": "extension.colorBarGreen",
				"title": "Color Title Bar : Green"
			},
			{
				"command": "extension.colorBarYellow",
				"title": "Color Title Bar : Yellow"
			},
			{
				"command": "extension.colorBarOrange",
				"title": "Color Title Bar : Orange"
			},
			{
				"command": "extension.colorBarPurple",
				"title": "Color Title Bar : Purple"
			},
			{
				"command": "extension.colorBarSilver",
				"title": "Color Title Bar : Silver"
			},
			{
				"command": "extension.colorBarKhaki",
				"title": "Color Title Bar : Khaki"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^8.10.25",
		"@types/mocha": "^2.2.42"
	}
}
