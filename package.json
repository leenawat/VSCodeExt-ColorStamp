{
	"name": "color-stamp",
	"displayName": "Color Stamp 2",
	"description": "Change VS Code window status bar color of each workspace for easy identifying",
	"version": "2.0.0",
	"publisher": "leenawat",
	"engines": {
		"vscode": "^1.55.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/leenawat/VSCodeExt-ColorStamp"
	},
	"license": "GPL-3.0",
	"categories": [
		"Other"
	],
	"keywords": [
		"title",
		"status",
		"bar",
		"id",
		"windows",
		"color",
		"stamp",
		"identify",
		"extension",
		"development",
		"UI",
		"vscode"
	],
	"activationEvents": [
		"onCommand:extension.vscode-title-color",
		"onCommand:extension.vscode-title-color-Blue",
		"onCommand:extension.vscode-title-color-Red",
		"onCommand:extension.vscode-title-color-Green",
		"onCommand:extension.vscode-title-color-Yellow",
		"onCommand:extension.vscode-title-color-Orange",
		"onCommand:extension.vscode-title-color-Purple",
		"onCommand:extension.vscode-title-color-Silver",
		"onCommand:extension.vscode-title-color-Khaki",
		"onCommand:extension.vscode-title-color-X"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.vscode-title-color",
				"title": "Title Color: ?"
			},
			{
				"command": "extension.vscode-title-color-Blue",
				"title": "Title Color: Blue"
			},
			{
				"command": "extension.vscode-title-color-Red",
				"title": "Title Color: Red"
			},
			{
				"command": "extension.vscode-title-color-Green",
				"title": "Title Color: Green"
			},
			{
				"command": "extension.vscode-title-color-Yellow",
				"title": "Title Color: Yellow"
			},
			{
				"command": "extension.vscode-title-color-Orange",
				"title": "Title Color: Orange"
			},
			{
				"command": "extension.vscode-title-color-Purple",
				"title": "Title Color: Purple"
			},
			{
				"command": "extension.vscode-title-color-Silver",
				"title": "Title Color: Silver"
			},
			{
				"command": "extension.vscode-title-color-Khaki",
				"title": "Title Color: Khaki"
			},
			{
				"command": "extension.vscode-title-color-X",
				"title": "Title Color: X"
			}
		]
	},
	"scripts": {
		"package": "vsce package",		
		"publish": "vsce publish",
		"vscode:prepublish": "npm run compile",
		"install": "code --install-extension color-stamp-2.0.0.vsix",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.55.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"typescript": "^4.1.3",
		"vsce": "^1.87.1",
		"vscode-test": "^1.5.0"
	}
}
